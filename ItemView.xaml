<?xml version="1.0" encoding="utf-8" ?>
<Border x:Class="CALayerLeak.ItemView"
        xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        xmlns:local="clr-namespace:CALayerLeak"
        x:Name="_itemView"
        x:DataType="x:Int32"
        BackgroundColor="#80808000"
        HeightRequest="50">
	<Border.StrokeShape>
		<RoundRectangle CornerRadius="{Binding CornerRadius, Source={Reference _itemView}, x:DataType={x:Type local:ItemView}}" />
	</Border.StrokeShape>
</Border>

